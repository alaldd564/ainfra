<?xml version="1.0" encoding="utf-8"?>
<merge xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    tools:layout_height="wrap_content"
    tools:layout_width="wrap_content"
    tools:orientation="vertical"
    tools:parentTag="android.widget.LinearLayout">

    <ImageView
        android:id="@+id/navermap_zoom_in"
        android:layout_width="46dp"
        android:layout_height="43dp"
        android:contentDescription="@string/navermap_zoom_in"
        android:src="@drawable/navermap_zoom_in" />

    <View
        android:layout_width="46dp"
        android:layout_height="1dp"
        android:background="@drawable/navermap_zoom_div" />

    <ImageView
        android:id="@+id/navermap_zoom_out"
        android:layout_width="46dp"
        android:layout_height="45dp"
        android:contentDescription="@string/navermap_zoom_out"
        android:src="@drawable/navermap_zoom_out" />

</merge>
